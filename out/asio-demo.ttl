#################################################################
#    Object Properties
#################################################################

###  http://www.asio.es/asioontologies/asio#advises
asio:advises rdf:type owl:ObjectProperty ;
             owl:inverseOf asio:isAdvisedBy ;
             rdfs:domain <http://www.asio.es/asioontologies/asio#Research&TeachingPersonnel> ;
             rdfs:range asio:Student .


###  http://www.asio.es/asioontologies/asio#capital
asio:capital rdf:type owl:ObjectProperty ;
             rdfs:domain <http://www.w3.org/2004/02/skos/core#Concept> ;
             rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
             rdfs:comment "A property to indicate that a place is the official seat of government in a political entity."@en .


###  http://www.asio.es/asioontologies/asio#codeOf
asio:codeOf rdf:type owl:ObjectProperty ;
            owl:inverseOf asio:hasCode ;
            rdfs:domain <http://www.w3.org/2004/02/skos/core#Concept> ;
            rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
            rdfs:comment "A code is assigned to a concept."@en .


###  http://www.asio.es/asioontologies/asio#country
asio:country rdf:type owl:ObjectProperty ;
             rdfs:domain <http://www.w3.org/2004/02/skos/core#Concept> ;
             rdfs:range asio:Country ;
             rdfs:comment "This property indicates the nationality of a resource."@en .


###  http://www.asio.es/asioontologies/asio#funds
asio:funds rdf:type owl:ObjectProperty ;
           owl:inverseOf asio:isFundedBy .


###  http://www.asio.es/asioontologies/asio#geo-division
asio:geo-division rdf:type owl:ObjectProperty ;
                  rdfs:domain <http://www.w3.org/2004/02/skos/core#Concept> ;
                  rdfs:range modules:GeopoliticalEntities ;
                  rdfs:comment "A property to link a resource to a predefined geographical (or geopolitical) division."@en .


###  http://www.asio.es/asioontologies/asio#hasCV
asio:hasCV rdf:type owl:ObjectProperty ;
           rdfs:domain foaf:Person ;
           rdfs:range asio:CV .


###  http://www.asio.es/asioontologies/asio#hasCode
asio:hasCode rdf:type owl:ObjectProperty ;
             rdfs:domain <http://www.w3.org/2004/02/skos/core#Concept> ;
             rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
             rdfs:comment "A concept can be asigned a code."@en .


###  http://www.asio.es/asioontologies/asio#hasScientificDomain
asio:hasScientificDomain rdf:type owl:ObjectProperty .


###  http://www.asio.es/asioontologies/asio#hasSealOfQuality
asio:hasSealOfQuality rdf:type owl:ObjectProperty ;
                      rdfs:range asio:ResearchInstitute .


###  http://www.asio.es/asioontologies/asio#hasTRL
asio:hasTRL rdf:type owl:ObjectProperty .


###  http://www.asio.es/asioontologies/asio#isAdvisedBy
asio:isAdvisedBy rdf:type owl:ObjectProperty .


###  http://www.asio.es/asioontologies/asio#isFundedBy
asio:isFundedBy rdf:type owl:ObjectProperty .


###  http://www.asio.es/asioontologies/asio#participant
asio:participant rdf:type owl:ObjectProperty ;
                 rdfs:range foaf:Agent .


###  http://www.asio.es/asioontologies/asio#requiredFor
asio:requiredFor rdf:type owl:ObjectProperty ;
                 rdfs:domain asio:AcademicPublication ;
                 rdfs:range asio:AcademicDegree .


###  http://www.asio.es/asioontologies/asio#requires
asio:requires rdf:type owl:ObjectProperty ;
              rdfs:domain asio:AcademicDegree ;
              rdfs:range asio:AcademicPublication .


###  http://www.asio.es/asioontologies/asio#spin-off
asio:spin-off rdf:type owl:ObjectProperty ;
              rdfs:domain asio:Company ,
                          asio:University ;
              rdfs:range asio:Spinoff .


###  http://xmlns.com/foaf/0.1/member
foaf:member rdf:type owl:ObjectProperty .


