#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.asio.es/asioontologies/asio#Publishes
asio:Publishes rdf:type owl:ObjectProperty ;
               owl:inverseOf asio:publishedBy .


###  http://www.asio.es/asioontologies/asio#addresses
asio:addresses rdf:type owl:ObjectProperty ;
               rdfs:domain asio:ResearchContribution ;
               rdfs:range asio:ResearchProblem ;
               rdfs:label "addresses"@en .


###  http://www.asio.es/asioontologies/asio#advises
asio:advises rdf:type owl:ObjectProperty ;
             owl:inverseOf asio:isAdvisedBy ;
             rdfs:domain <http://www.asio.es/asioontologies/asio#Research&TeachingPersonnel> ;
             rdfs:range asio:Student ;
             rdfs:label "advises"@en .


###  http://www.asio.es/asioontologies/asio#atContext
asio:atContext rdf:type owl:ObjectProperty ;
               rdfs:domain asio:Profile ;
               rdfs:range asio:Context ;
               rdfs:comment "This property enables to relate a profile with a given context (geographical, temporal, spatial, social, etc.)."@en ;
               rdfs:label "at context"@en .


###  http://www.asio.es/asioontologies/asio#authors
asio:authors rdf:type owl:ObjectProperty ;
             owl:inverseOf asio:isAuthoredBy ;
             rdfs:label "authors"@en .


###  http://www.asio.es/asioontologies/asio#capital
asio:capital rdf:type owl:ObjectProperty ;
             rdfs:domain skos:Concept ;
             rdfs:range skos:Concept ;
             rdfs:comment "A property to indicate that a place is the official seat of government in a political entity."@en ;
             rdfs:label "capital"@en .


###  http://www.asio.es/asioontologies/asio#codeOf
asio:codeOf rdf:type owl:ObjectProperty ;
            owl:inverseOf asio:hasCode ;
            rdfs:domain skos:Concept ;
            rdfs:range skos:Concept ;
            rdfs:comment "A code is assigned to a concept."@en ;
            rdfs:label "code of"@en .


###  http://www.asio.es/asioontologies/asio#country
asio:country rdf:type owl:ObjectProperty ;
             rdfs:domain skos:Concept ;
             rdfs:range asio:Country ;
             rdfs:comment "This property indicates the nationality of a resource."@en ;
             rdfs:label "country"@en ,
                        "país"@es .


###  http://www.asio.es/asioontologies/asio#employs
asio:employs rdf:type owl:ObjectProperty ;
             rdfs:domain asio:ResearchContribution ;
             rdfs:range asio:ResearchMethod ;
             rdfs:label "emplea"@es ,
                        "employs"@en .


###  http://www.asio.es/asioontologies/asio#funds
asio:funds rdf:type owl:ObjectProperty ;
           owl:inverseOf asio:isFundedBy ;
           rdfs:label "funds"@en .


###  http://www.asio.es/asioontologies/asio#geo-division
asio:geo-division rdf:type owl:ObjectProperty ;
                  rdfs:domain skos:Concept ;
                  rdfs:range modules:GeopoliticalEntities ;
                  rdfs:comment "A property to link a resource to a predefined geographical (or geopolitical) division."@en ;
                  rdfs:label "geo-division"@en .


###  http://www.asio.es/asioontologies/asio#hasCV
asio:hasCV rdf:type owl:ObjectProperty ;
           rdfs:domain foaf:Person ;
           rdfs:range asio:CV ;
           rdfs:label "has CV"@en .


###  http://www.asio.es/asioontologies/asio#hasCode
asio:hasCode rdf:type owl:ObjectProperty ;
             rdfs:domain skos:Concept ;
             rdfs:range skos:Concept ;
             rdfs:comment "A concept can be asigned a code."@en ;
             rdfs:label "has Code"@en .


###  http://www.asio.es/asioontologies/asio#hasPatent
asio:hasPatent rdf:type owl:ObjectProperty ;
               rdfs:range asio:Patent ;
               rdfs:label "has patent"@en .


###  http://www.asio.es/asioontologies/asio#hasResearchContribution
asio:hasResearchContribution rdf:type owl:ObjectProperty ;
                             rdfs:label "has research contribution"@en .


###  http://www.asio.es/asioontologies/asio#hasResearchField
asio:hasResearchField rdf:type owl:ObjectProperty ;
                      rdfs:label "has research field"@en .


###  http://www.asio.es/asioontologies/asio#hasScientificDomain
asio:hasScientificDomain rdf:type owl:ObjectProperty ;
                         rdfs:label "has scientific domain"@en .


###  http://www.asio.es/asioontologies/asio#hasSealOfQuality
asio:hasSealOfQuality rdf:type owl:ObjectProperty ;
                      rdfs:range asio:ResearchInstitute ;
                      rdfs:label "has seal of quality"@en .


###  http://www.asio.es/asioontologies/asio#hasTRL
asio:hasTRL rdf:type owl:ObjectProperty ;
            rdfs:label "has TRL"@en .


###  http://www.asio.es/asioontologies/asio#isAdvisedBy
asio:isAdvisedBy rdf:type owl:ObjectProperty ;
                 rdfs:label "is advised by"@en .


###  http://www.asio.es/asioontologies/asio#isAuthoredBy
asio:isAuthoredBy rdf:type owl:ObjectProperty ;
                  rdfs:label "is authored by"@en .


###  http://www.asio.es/asioontologies/asio#isFundedBy
asio:isFundedBy rdf:type owl:ObjectProperty ;
                rdfs:label "is funded by"@en .


###  http://www.asio.es/asioontologies/asio#isMemberOf
asio:isMemberOf rdf:type owl:ObjectProperty ;
                owl:inverseOf foaf:member ;
                rdfs:label "is member of"@en .


###  http://www.asio.es/asioontologies/asio#participant
asio:participant rdf:type owl:ObjectProperty ;
                 rdfs:range foaf:Agent ;
                 rdfs:label "participant"@en .


###  http://www.asio.es/asioontologies/asio#participates
asio:participates rdf:type owl:ObjectProperty ;
                  rdfs:comment "This property encodes the participation relation between profiles and the communities they are member of."@en ;
                  rdfs:label "participates"@en .


###  http://www.asio.es/asioontologies/asio#personalizes
asio:personalizes rdf:type owl:ObjectProperty ;
                  rdfs:domain asio:Profile ;
                  rdfs:range asio:Role ;
                  rdfs:comment "A role is personalized by means of a profile, which wrap the role in the identity of the person behind the profile."@en ;
                  rdfs:label "personalizes"@en .


###  http://www.asio.es/asioontologies/asio#plays
asio:plays rdf:type owl:ObjectProperty ;
           rdfs:domain foaf:Agent ;
           rdfs:range asio:Role ;
           rdfs:comment "This property relates roles and their players, who can be any foaf:Agent (a person, a group, etc.)"@en ;
           rdfs:label "plays"@en .


###  http://www.asio.es/asioontologies/asio#profiles
asio:profiles rdf:type owl:ObjectProperty ;
              rdfs:domain foaf:Agent ;
              rdfs:range asio:Profile ;
              rdfs:comment "This property relates people and the featured profiles."@en ;
              rdfs:label "profiles"@en .


###  http://www.asio.es/asioontologies/asio#publishedBy
asio:publishedBy rdf:type owl:ObjectProperty .


###  http://www.asio.es/asioontologies/asio#requiredFor
asio:requiredFor rdf:type owl:ObjectProperty ;
                 rdfs:domain asio:AcademicPublication ;
                 rdfs:range asio:AcademicDegree ;
                 rdfs:label "requiredFor"@en .


###  http://www.asio.es/asioontologies/asio#requires
asio:requires rdf:type owl:ObjectProperty ;
              rdfs:domain asio:AcademicDegree ,
                          asio:Role ;
              rdfs:range asio:AcademicPublication ,
                         asio:Role ;
              rdfs:comment "This property relates roles and their requisites, i.e. other roles."@en ;
              rdfs:label "requires"@en .


###  http://www.asio.es/asioontologies/asio#researchesIn
asio:researchesIn rdf:type owl:ObjectProperty ;
                  rdfs:domain foaf:Agent ;
                  rdfs:range modules:ScientificDomain ;
                  rdfs:label "researches in"@en .


###  http://www.asio.es/asioontologies/asio#specializes
asio:specializes rdf:type owl:ObjectProperty ;
                 rdfs:domain asio:Role ;
                 rdfs:range asio:Role ;
                 rdfs:comment "This property allows to express hierarchies of roles, through of role specialization: if role A specializes role B, and B specializes C, then role A specializes role C."@en ;
                 rdfs:label "specializes"@en .


###  http://www.asio.es/asioontologies/asio#spinsOffFrom
asio:spinsOffFrom rdf:type owl:ObjectProperty ;
                  rdfs:domain asio:Spinoff ;
                  rdfs:label "spins of from"@en .


###  http://www.asio.es/asioontologies/asio#supervised
asio:supervised rdf:type owl:ObjectProperty ;
                owl:inverseOf asio:supervisedBy ;
                rdfs:label "supervised"@en .


###  http://www.asio.es/asioontologies/asio#supervisedBy
asio:supervisedBy rdf:type owl:ObjectProperty ;
                  rdfs:label "supervised by"@en .


###  http://www.asio.es/asioontologies/asio#utilizes
asio:utilizes rdf:type owl:ObjectProperty ;
              rdfs:domain asio:ResearchContribution ;
              rdfs:range asio:ResearchMaterial ;
              rdfs:label "utilizes"@en .


###  http://www.asio.es/asioontologies/asio#yields
asio:yields rdf:type owl:ObjectProperty ;
            rdfs:domain asio:ResearchContribution ;
            rdfs:range asio:ResearchResult ;
            rdfs:label "yields"@en .


###  http://xmlns.com/foaf/0.1/member
foaf:member rdf:type owl:ObjectProperty ;
            rdfs:label "member"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.asio.es/asioontologies/asio#isActive
asio:isActive rdf:type owl:DatatypeProperty ;
              rdfs:domain asio:Profile ;
              rdfs:range xsd:boolean ;
              rdfs:comment """This property allows to specify which is the user profile that is active, 
given a interaction between two applications."""@en ;
              rdfs:label "is active"@en .


###  http://www.asio.es/asioontologies/asio#projectEndDate
asio:projectEndDate rdf:type owl:DatatypeProperty ;
                    rdfs:domain foaf:Project ;
                    rdfs:comment "The end date of the project."@en ;
                    rdfs:label "fecha final proyecto"@es ,
                               "project end date"@en .


###  http://www.asio.es/asioontologies/asio#projectKeyword
asio:projectKeyword rdf:type owl:DatatypeProperty ;
                    rdfs:domain foaf:Project ;
                    rdfs:comment "A key word of the project."@en ;
                    rdfs:label "palabra clave"@es ,
                               "project keyword"@en .


###  http://www.asio.es/asioontologies/asio#projectObjective
asio:projectObjective rdf:type owl:DatatypeProperty ;
                      rdfs:domain foaf:Project ;
                      rdfs:comment "A defined objective of the project."@en ;
                      rdfs:label "objetivo de proyecto"@es ,
                                 "project objective"@en .


###  http://www.asio.es/asioontologies/asio#projectStartDate
asio:projectStartDate rdf:type owl:DatatypeProperty ;
                      rdfs:domain foaf:Project ;
                      rdfs:comment "The start date of the project."@en ;
                      rdfs:label "fecha inicio proyecto"@es ,
                                 "project start date"@en .


###  http://www.asio.es/asioontologies/asio#publicationDate
asio:publicationDate rdf:type owl:DatatypeProperty ;
                     rdfs:label "publication date"@en .


