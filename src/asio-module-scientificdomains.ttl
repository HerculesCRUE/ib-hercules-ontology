asioModules:ES_SCIENTIFIC_DOMAIN_LEVEL_2_QMC
      a       skos:Concept ;
      rdfs:label "Química" ;
      asio:hasCode asioModules:ES_SCIENTIFIC_DOMAIN_LEVEL_2_CODE_QMC ;
