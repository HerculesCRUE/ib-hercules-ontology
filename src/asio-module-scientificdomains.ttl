asioModules:ES_SCIENTIFIC_DOMAIN_LEVEL_2_BFS
      a       skos:Concept ;
      rdfs:label "Biología fundamental y de sistemas" ;
      asio:hasCode asioModules:ES_SCIENTIFIC_DOMAIN_LEVEL_2_CODE_BFS ;
      skos:broader asioModules:ES_SCIENTIFIC_DOMAIN_LEVEL_1_CVS ;
